#include <iostream>
#include <vector>

/*функция push_back(), в которую передается добавляемый элемент*/

int main()
{
    std::vector<int> numbers;     // пустой вектор
    numbers.push_back(5);
    numbers.push_back(536);
    numbers.push_back(75);
    numbers.push_back(946);
    for (int i = 0; i < numbers.size(); i++)
        std::cout << numbers[i] << "\t";      
}
